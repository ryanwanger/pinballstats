<table>
  <thead>
    <tr>
      <th>Your league</th>
      <th>Start date</th>
    </tr>
  </thead>

  <tbody>
    <% @leagues.order("id desc").each do |league| %>
      <tr data-link="<%= league_path(league)%>">
        <td><%= league.title %></td>
        <td><%= league.start_date.strftime("%B %-d, %Y") %></td>
      </tr>
    <% end %>
  </tbody>
</table>